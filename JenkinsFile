pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'From GitHub'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deploy to AWS'
            }
        }
        stage('Test') {
             steps {
                 echo 'sonar Tests'
             }
         }
          stage('Deploy TEST') {
              steps {
                  echo 'deploy to TEST Env'
              }
          }
         stage('Deploy PROD') {
             steps {
                 echo 'deploy to Production'
             }
         }

    }
}
